{
  "version": 2,
  "builds": [
    {
      "src": "api/index.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.go"
    },
    {
      "src": "/health",
      "dest": "/api/index.go"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.go"
    }
  ],
  "env": {
    "GO_VERSION": "1.21"
  },
  "functions": {
    "api/index.go": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
